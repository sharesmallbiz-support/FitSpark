{
  "name": "fitspark-client",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:api": "dotnet build FitSpark.Api/FitSpark.Api.csproj",
    "build:all": "npm run build && npm run build:api",
    "build:production": "npm run build && dotnet build FitSpark.Api/FitSpark.Api.csproj --configuration Release",
    "start:api": "dotnet run --project FitSpark.Api/FitSpark.Api.csproj",
    "start:dev": "dotnet run --project FitSpark.Api/FitSpark.Api.csproj",
    "start:production": "npm run build && cd FitSpark.Api && dotnet run --configuration Release",
    "docker:build": "docker build -t fitspark .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "deploy": "npm run build && dotnet publish FitSpark.Api/FitSpark.Api.csproj -c Release -o ./publish",
    "clean": "rimraf FitSpark.Api/wwwroot && dotnet clean FitSpark.Api/FitSpark.Api.csproj",
    "restore": "npm ci && dotnet restore FitSpark.Api/FitSpark.Api.csproj",
    "preview": "vite preview",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.85.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.539.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "wouter": "^3.7.1",
    "zod": "^4.0.17",
    "zod-validation-error": "^4.0.1"
  },
  "devDependencies": {
